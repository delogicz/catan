package jsonstructures;

import java.util.ArrayList;

import model.ResourceType;
/**
 * Wrapper for the ErfindungDaten
 * @author Felixi, Georg
 *
 */
public class ErfindungNachricht {
	private ErfindungDaten Erfindung;
	/**
	 * Constructor
	 * @param erfindung
	 */
	public ErfindungNachricht(ArrayList<ResourceType> erfindung) {
		Erfindung = new ErfindungDaten(erfindung);
	}
	
	/**
	 * Returns ErfindungDaten
	 * @return ErfindungDaten
	 */
	public ErfindungDaten getErfindung() {
		return Erfindung;
	}
	
	
}
