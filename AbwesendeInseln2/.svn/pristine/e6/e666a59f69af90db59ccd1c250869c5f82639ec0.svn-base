package jsonstructures;

/**
 * This class containes the version of the Client or the server
 * @author Felixi, Georg 
 *
 */
public class VersionDaten {
	private String Version;
	
	/**
	 * Creates a VersionDaten object
	 * @param version		version of the client or server
	 */
	public VersionDaten(String version){
		this.Version = version;
	}
	
	/**
	 * Getter for Version
	 * @return
	 */
	
	public String getVersion(){
		return Version;
	}
}
