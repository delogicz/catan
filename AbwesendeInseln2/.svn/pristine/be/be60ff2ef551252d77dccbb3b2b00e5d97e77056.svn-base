package jsonstructures;

/**
 * This class is the wrapper class for the ServerChatDaten class
 * @author Felixi, Georg
 *
 */
public class ServerChatNachricht {
	private ServerChatDaten Chatnachricht;

	/**
	 * Creates a ServerChatDaten object
	 * @param absender		id of the player
	 * @param nachricht		message
	 */
	public ServerChatNachricht(int absender, String nachricht) {
		Chatnachricht = new ServerChatDaten(absender, nachricht);
	}

	/**
	 * Getter for Chatnachricht
	 * @return
	 */
	public ServerChatDaten getDaten() {
		return Chatnachricht;
	}
	
	
}
