package model;

/**
 * Exception that is thrown when someone tries to create another Server 
 * on a port that is already in use
 * 
 * @author Stefanie Kloss
 */
public class PortAlreadyInUseException extends Exception {

	/**
	 * SerialID of PortAlreadyInUseException
	 */
	private static final long serialVersionUID = -8810898274398614632L;

	/**
	 * Constructor
	 */
	public PortAlreadyInUseException(){
		super("Port is already in use!");
	}
}
