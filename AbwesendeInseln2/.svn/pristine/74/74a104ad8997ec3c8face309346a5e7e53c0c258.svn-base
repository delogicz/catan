package jsonstructures;

/**
 * This class is for the client to receive his id from the server
 * @author Felixi, Georg
 *
 */
public class idDaten {
	private int id;
	
	
	/**
	 * Creates an idDaten object
	 * @param id		the id of the player
	 */
	public idDaten(int id){
		this.id = id;
	}

	/**
	 * Getter for ID
	 * @return
	 */
	public int getDaten() {
		return id;
	}
}
