package model;

/**
 * All possible results of checking for the owner of the longest road
 * 
 * @author Stefanie Kloss
 */
public enum LongestRoadChanges {
	/** no one has the longestRoad anymore*/
	NO_ONE,
	/** new player owns the longestRoad now*/
	NEW_OWNER,
	/** owner of longestRoad stays the same*/
	SAME_OWNER
}
