package jsonstructures;


/**
 * This class containes the message that the server sends if an error occured in FehlerNachricht
 * @author Felixi, Georg
 *
 */
public class FehlerDaten {
	private String Meldung;
	
	/**
	 * creates a FehlerDaten objects 
	 * @param meldung	message in the error
	 */

	public FehlerDaten(String meldung) {
		Meldung = meldung;
	}
	
	/**
	 * Getter for Meldung
	 * @return
	 */
	public String getMeldung(){
		return Meldung;
	}
}
