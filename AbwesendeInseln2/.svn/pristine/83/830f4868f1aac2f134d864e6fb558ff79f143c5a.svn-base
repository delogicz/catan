package model;

/**
 * Exception that is thrown whenever something goes wrong
 * playing a development card
 * @author Stefanie Kloss
 */
public class DevCardException extends Exception {

	/**
	 * SerialID of DevCardException
	 */
	private static final long serialVersionUID = -5360699275691162233L;
	
	/**
	 * Constructor
	 * @param message message to user about what's going wrong
	 */
	public DevCardException(String message){
		super(message);
	}
}
