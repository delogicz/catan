package model;

/**
 * Exception that is thrown when Client is trying to do something but has 
 * the wrong status
 * @author Stefanie Kloss
 */
public class WrongStatusException extends Exception {
	
	/**
	 * SerialID of WrongStatusException
	 */
	private static final long serialVersionUID = 4134700713884425025L;
	
	/**
	 * Constructor
	 */
	public WrongStatusException(){
		super("You cannot do this action right now.\nPlease pay attention to your status.");
	}
}
