package jsonstructures;

/**
 * Wrapper for ErtragUnbekanntDaten
 * @author Felixi, Georg
 *
 */
public class ErtragUnbekanntNachricht {
	private ErtragUnbekanntDaten Ertrag;

	/**
	 * Constructor
	 * @param spieler
	 * @param anzahl
	 */
	public ErtragUnbekanntNachricht(int spieler, int anzahl) {
		Ertrag = new ErtragUnbekanntDaten(spieler,anzahl);
	}

	/**
	 * Getter for Ertrag
	 * @return
	 */
	public ErtragUnbekanntDaten getErtrag() {
		return Ertrag;
	}
	
	
}
